#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bihuaizhi/Documents/duomaiCode/company_component_next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bihuaizhi/Documents/duomaiCode/company_component_next/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/dumi@1.1.38_react@16.12.0/node_modules/dumi/bin/dumi.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/dumi@1.1.38_react@16.12.0/node_modules/dumi/bin/dumi.js" "$@"
fi
